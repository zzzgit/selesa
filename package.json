{
  "name": "selesa",
  "version": "0.0.28",
  "description": "a npm package for .bashrc synchronization",
  "repository": {
    "type": "git",
    "url": "https://github.com/zzzgit/selesa"
  },
  "main": "index.js2",
  "scripts": {
    "touch": "shx touch ~/.selesarc",
    "install": "echo  ",
