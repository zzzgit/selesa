{
  "name": "selesa",
  "version": "0.0.7",
  "description": "a npm package for .bashrc synchronization",
  "repository": {
    "type": "git",
    "url": "https://github.com/zzzgit/selesa"
  },
  "main": "index.js2",
  "scripts": {
    "check": "node ./build/ensure.env.js",
    "touch": "shx touch ~/.selesarc",
    "install": "npm run touch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uninstall": "shx rm ~/.selesarc && node ./uninstall.js"
  },
  "bugs": "https://github.com/zzzgit/selesa/issues",
  "keywords": [
    "shell",
    "terminal"
  ],
  "bin": {
    "selesa": "./bin/main.js"
  },
  "engines": {
    "npm": ">=6.1.0",
    "node": ">=10.5"
  },
  "files": [
    "bin/",
    "rc/"
  ],
  "author": "jonkee",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^7.0.0",
    "ini": "^1.3.5",
    "log4js": "^3.0.6",
    "shx": "^0.3.2",
    "yargs": "^12.0.1"
  }
}
