{
  "name": "selesa",
  "version": "26.0.2",
  "description": "a npm package for .bashrc synchronization",
  "repository": {
    "type": "git",
    "url": "https://github.com/zzzgit/selesa"
  },
  "type": "module",
  "scripts": {
    "touch": "shx touch ~/.config/selesa/config.ini",
    "install": "echo  ",
    "test": "dotenv -e .env.test -- jasmine",
    "uninstall": "shx rm ~/.config/selesa/config.ini && shx rm -rf ~/.selesa && node ./uninstall.js"
  },
  "bugs": "https://github.com/zzzgit/selesa/issues",
  "keywords": [
    "shell",
    "terminal",
    "sync"
  ],
  "bin": {
    "selesa": "./bin/main.js"
  },
  "engines": {
    "npm": ">=10",
    "node": ">=22"
  },
  "author": "jonkee",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "dakini": "^1.0.5",
    "ini": "^6.0.0",
    "log4js": "^6.9.1",
    "nodemailer": "^7.0.11",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-janus": "^9.0.20",
    "eslint-plugin-mocha": "^11.2.0",
    "jasmine": "^5.13.0",
    "shx": "^0.4.0"
  }
}
